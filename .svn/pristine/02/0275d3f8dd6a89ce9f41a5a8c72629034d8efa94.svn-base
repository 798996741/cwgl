    //新增
    function add() {
        var csname = $("#csname").val();
        var time = $("#time").val();
        //top.jzts();
        var winId = "userWin";
        if (csname != null && csname != '' && time != null && time != '') {
            modals.openWin({
                winId: winId,
                title: '新增',
                width: '900px',
                url: '<%=basePath%>reimdet/goAddC.do?csname=' + encodeURI(encodeURI(csname)) + '&time=' + encodeURI(encodeURI(time))
