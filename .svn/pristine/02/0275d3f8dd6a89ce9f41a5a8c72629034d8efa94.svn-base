                            <select class="seat-select" name="csname" id="csname" data-placeholder="公司"
                                    onchange="ntchange();">
                                <option value="" selected></option>
                                <c:forEach items="${getcsname}" var="csname" varStatus="vs">
                                    <option value="${csname.csname}"
