                        </tr>
                        </thead>
                        <tbody>

                        <c:choose>
                            <c:when test="${not empty relist}">
                                <%--<c:if test="${QX.cha == 1 }">--%>
                                <c:forEach items="${relist}" var="relist" varStatus="vs">


                                    <tr>

                                        <td>${vs.count}</td>
