package com.fh.pojo;

import java.io.Serializable;
import java.math.BigDecimal;

public class tbinvmon implements Serializable{
    private Long invmid;
    private Long conid;
    private String invtime;
    private BigDecimal invamon;
    private String invcontent;
    private String tax;
    private BigDecimal invnmon;
    private BigDecimal paymon;
    private String username;
    private String cztime;

    @Override
    public String toString() {
        return "tbinvmon{" +
                "invmid=" + invmid +
                ", conid=" + conid +
                ", invtime='" + invtime + '\'' +
                ", invamon=" + invamon +
                ", invcontent='" + invcontent + '\'' +
                ", tax=" + tax +
                ", invnmon=" + invnmon +
                ", paymon=" + paymon +
                ", username='" + username + '\'' +
                ", cztime='" + cztime + '\'' +
                '}';
    }

    public Long getInvmid() {
        return invmid;
    }

    public void setInvmid(Long invmid) {
        this.invmid = invmid;
    }

    public Long getConid() {
        return conid;
    }

    public void setConid(Long conid) {
        this.conid = conid;
    }

    public String getInvtime() {
        return invtime;
    }

    public void setInvtime(String invtime) {
        this.invtime = invtime;
    }

    public BigDecimal getInvamon() {
        return invamon;
    }

    public void setInvamon(BigDecimal invamon) {
        this.invamon = invamon;
    }

    public String getInvcontent() {
        return invcontent;
    }

    public void setInvcontent(String invcontent) {
        this.invcontent = invcontent;
    }

    public String getTax() {
        return tax;
    }

    public void setTax(String tax) {
        this.tax = tax;
    }

    public BigDecimal getInvnmon() {
        return invnmon;
    }

    public void setInvnmon(BigDecimal invnmon) {
        this.invnmon = invnmon;
    }

    public BigDecimal getPaymon() {
        return paymon;
    }

    public void setPaymon(BigDecimal paymon) {
        this.paymon = paymon;
    }

    public String getUsername() {
        return username;
    }

    public void setUsername(String username) {
        this.username = username;
    }

    public String getCztime() {
        return cztime;
    }

    public void setCztime(String cztime) {
        this.cztime = cztime;
    }
}
