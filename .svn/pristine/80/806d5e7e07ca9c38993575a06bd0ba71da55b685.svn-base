/*锟斤拷选锟斤拷CSS*/
.radioCheck {
    width: 20px;
    height: 20px;
    margin: 0 auto;
}

.radioCheck label {
    margin-left: 50px;
    margin-right: 50px;
}

.radioCheck label font {
    font-size: 16px;
    line-height: 47px;
    padding-left: 4px;
}

input[type='radio'] {
    appearance: none;
    -webkit-appearance: none;
    outline: none;
    display: none;
}

label input[type='radio'] + span {
    width: 13px;
    height: 13px;
    display: inline-block;
    background: url("../img/home/noselect_icon.png") no-repeat;
}

label input[type='radio']:checked + span {
    width: 13px;
    height: 13px;
