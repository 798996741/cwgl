@media screen and (max-width: 1920px) {
  body {
    overflow-y: hidden;
  }

  .sidebar-menu > li:hover > .treeview-menu {
    top: 41px;
  }

  .skin-blue .main-header .navbar .nav > li > a,
  .seat-middle-r table tbody tr td,
  .seat-page-l,
  .seat-page-r-li li {
    font-size: 13px;
  }

  .seat-nav,
  .skin-blue .sidebar-menu > li > a,
  .treeview span,
  .seat-middle-l li,
  .seat-middle-r table thead tr th {
    font-size: 13px;
    font-weight: bold;
  }
  .seat-tab li a {
    font-size: 14px;
    font-weight: bold;
  }
  .seat-title-img {
    width: 15px;
    height: 15px;
  }
  .sidebar-menu > li > a {
    padding: 13px;
  }

  .seat-middle-l li {
    line-height: 40px;
  }

  .seat-middle-l li,
  .seat-middle-r table thead tr,
  .seat-middle-r table tbody tr {
    height: 36px;
  }

  .seat-middle-l {
    width: 75px;
  }

  .seat-middle-r {
    width: 100%;
  }

  .seat-middle-nr {
    height: 480px;
    width: 1345px;
  }

  .seat-middle {
    width: 1345px;
  }

  .content-top {
    width: 1345px;
  }

  .content-top-1 {
    width: 1345px;
  }

  .content-top-2 {
    width: 1345px;
  }

  .TEtable-title {
    width: 1345px;
  }

  .content {
    padding: 10px 15px;
  }

  .seat-tab {
    height: 36px;
  }

  .seat-tab li {
    height: 36px;
    line-height: 36px;
  }
  .user {
    font-size: 13px;
  }
  .user input {
    font-size: 13px;
  }
  .border-size {
    font-size: 13px;
  }
  .radio-size {
    font-size: 13px;
  }
  label input[type="radio"] + span {
    width: 13px;
    height: 13px;
  }
  .border-style {
    width: 189px;
  }
  .width-24px {
    width: 32px;
  }
  .width-278px {
    width: 280px;
  }
  .content-top-left {
    width: 850px;
  }
  .seat-select {
    width: 140px;
  }
}

